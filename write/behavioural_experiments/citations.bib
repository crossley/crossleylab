	keywords = {Computer Science - Computer Vision and Pattern Recognition, Computer Science - Machine Learning, Computer Science - Neural and Evolutionary Computing, Quantitative Biology - Neurons and Cognition},
	file = {Ivanov and Michmizos - 2021 - Increasing Liquid State Machine Performance with E.pdf:/Users/mq20185996/Zotero/storage/L9D3F97K/Ivanov and Michmizos - 2021 - Increasing Liquid State Machine Performance with E.pdf:application/pdf},
}

@article{adler_temporal_2012,
	title = {Temporal {Convergence} of {Dynamic} {Cell} {Assemblies} in the {Striato}-{Pallidal} {Network}},
