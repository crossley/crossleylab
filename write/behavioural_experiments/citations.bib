	file = {Barron et al. - 2015 - Decision-making and action selection in insects i.pdf:/Users/mq20185996/Zotero/storage/D4FLDGQI/Barron et al. - 2015 - Decision-making and action selection in insects i.pdf:application/pdf},
}

@article{tomkins_transient_2014,
