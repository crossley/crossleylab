	number = {1},
	urldate = {2024-11-20},
	journal = {The Journal of Thoracic and Cardiovascular Surgery},
	author = {Nathan, Meena and Sadhwani, Anjali and Gauvreau, Kimberlee and Agus, Michael and Ware, Janice and Newburger, Jane W. and Pigula, Frank},
	month = jul,
	year = {2014},
	pages = {232--237.e3},
	file = {Nathan et al. - 2014 - Association between Technical Performance Scores a.pdf:/Users/mq20185996/Zotero/storage/BLR89TQ5/Nathan et al. - 2014 - Association between Technical Performance Scores a.pdf:application/pdf},
}

