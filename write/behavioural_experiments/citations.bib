	pages = {107941},
	file = {08cb745410b014d65d67c31ab3ec957ee9b7.pdf:/Users/mq20185996/Zotero/storage/GKFMID5N/08cb745410b014d65d67c31ab3ec957ee9b7.pdf:application/pdf;7644.full.pdf:/Users/mq20185996/Zotero/storage/Z4JSG8GF/7644.full.pdf:application/pdf;19900013628.pdf:/Users/mq20185996/Zotero/storage/36RMEGAI/19900013628.pdf:application/pdf;Broschard et al. - 2024 - Disrupting dorsal hippocampus impairs category lea.pdf:/Users/mq20185996/Zotero/storage/F4ABZS74/Broschard et al. - 2024 - Disrupting dorsal hippocampus impairs category lea.pdf:application/pdf;document.pdf:/Users/mq20185996/Zotero/storage/2EDFTM38/document.pdf:application/pdf;s00221-002-1089-6.pdf:/Users/mq20185996/Zotero/storage/2U9BN7FP/s00221-002-1089-6.pdf:application/pdf},
